    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "np.random.seed(0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "import matplotlib.pylab as plt\n",
    "%matplotlib inline"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "for i in range(4):\n",
    "    mu = 0\n",
    "    sigma = 1\n",
    "    x = np.random.normal(mu, sigma, 100)\n",
    "    y = []\n",
    "    for j in range(100):\n",
    "        generated = np.random.logistic()\n",
    "        if generated > 0:\n",
    "            sign = +1\n",
    "        else:\n",
    "            sign = -1\n",
    "        xj = x[j]\n",
    "        z = xj*sign\n",
    "        y.append(z)\n",
    "    intercept, slope = np.polynomial.polynomial.polyfit(x, y, 1)\n",
    "    abline_values = [slope * i + intercept for i in x]\n",
    "    plt.figure(i)\n",
    "    plt.subplot()\n",
    "    plt.scatter(x,y)\n",
    "    plt.scatter(x,abline_values)"
   ]
  },
